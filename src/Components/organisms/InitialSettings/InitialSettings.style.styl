.wrapper
  display flex
  flex-direction column
  transition all 0.7s ease 0.2s
  overflow hidden
  height 100vh
  width 100vw
  &.notStarted
    transform translateX(100vw)
.slideWrapper
  flex 1
  display flex
  flex-direction column
  justify-self center
  padding-bottom 25px
.progressBar
  margin 0 auto